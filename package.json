{
  "name": "crime-data-explorer",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "6.9.x"
  },
  "scripts": {
    "start": "node build/server.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack -p --progress --config webpack.client.config.js",
    "build:server": "webpack --config webpack.server.config.js",
    "watch": "npm run watch:server & npm run watch:client",
    "watch:server": "webpack -w --config webpack.server.config.js & nodemon build/server.js",
    "watch:client": "webpack -w --progress --config webpack.client.config.js",
    "lint": "npm run lint:js && npm run lint:yml",
    "lint:js": "eslint src scripts server.js || exit 0",
    "lint:yml": "yamllint content/**/*.yml || exit 0",
    "test": "jest --env=jsdom --no-cache",
    "test:debug": "node --debug-brk jest -i --env=jsdom",
    "test:watch": "jest --env=jsdom --watch",
    "coverage": "npm test -- --coverage"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "babel-polyfill": "^6.20.0",
    "basscss-sass": "^4.0.0",
    "cfenv": "^1.0.3",
    "d3-array": "^1.0.2",
    "d3-collection": "^1.0.2",
    "d3-format": "^1.0.2",
    "d3-geo": "^1.4.0",
    "d3-scale": "^1.0.4",
    "d3-shape": "^1.0.4",
    "d3-time-format": "^2.0.3",
    "express": "^4.14.0",
    "glossary-panel": "^0.2.0",
    "lodash.debounce": "^4.0.8",
    "lodash.flatten": "^4.4.0",
    "lodash.lowercase": "^4.3.0",
    "lodash.range": "^3.2.0",
    "lodash.startcase": "^4.4.0",
    "lodash.throttle": "^4.1.1",
    "lodash.upperfirst": "^4.3.1",
    "markdown-it": "^8.2.2",
    "newrelic": "^1.35.1",
    "normalize.css": "^5.0.0",
    "pluralize": "^3.1.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.1",
    "redux-thunk": "^2.1.0",
    "source-map-support": "^0.4.11",
    "topojson": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.4",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^17.0.2",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "css-loader": "^0.26.1",
    "enzyme": "^2.6.0",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "jest": "^17.0.3",
    "js-yaml": "^3.7.0",
    "json-loader": "^0.5.4",
    "node-sass": "^3.13.0",
    "nodemon": "^1.11.0",
    "postcss-loader": "^1.2.1",
    "react-addons-test-utils": "^15.4.1",
    "sass-loader": "^4.0.2",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "webpack": "1.13.2",
    "yaml-lint": "0.0.4",
    "yaml-loader": "^0.4.0"
  }
}
